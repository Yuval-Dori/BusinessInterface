@model BusinessesInterface.Models.Address

@{
    ViewBag.Title = "List of Items";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Usage History</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Devices.FirstOrDefault().Id)
        </th>
                <th>
            @Html.DisplayNameFor(model => model.Devices.FirstOrDefault().HistoryDateTime)
        </th>
        <th></th>
    </tr>

    @foreach (var device in Model.Devices)
    {
        if(device.HistoryDateTime == null)
        {
            continue;
        }

        @foreach(var time in device.HistoryDateTime)
        {
        <tr>
        <td>
        @Html.DisplayFor(modelItem => device.Id)
        </td>
         <td>
        @Html.DisplayFor(modelItem => time)
        </td>
        </tr>
        }
    }

</table>
<div>
  @Html.ActionLink("Search By Socket Number", "SearchSocket")
</div>
<script src="~/bundles/jqueryval"></script>
